{
	"name": "aw-studio/installer",
	"description": "Laravel application and dependency installer.",
	"license": "MIT",
	"authors": [
		{
			"name": "Lennart Carstens-Behrens",
			"email": "lennart.carbe@gmail.com"
		}
	],
	"bin": [
		"bin/aw"
	],
	"autoload": {
		"psr-4": {
