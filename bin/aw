#!/usr/bin/env php
<?php

if (file_exists(__DIR__.'/../../../autoload.php')) {
    require __DIR__.'/../../../autoload.php';
} else {
    require __DIR__.'/../vendor/autoload.php';
}

$app = new Symfony\Component\Console\Application('AwStudio Installer', '1.0');

$app->add(new AwStudio\Installer\Commands\NewCommand);

$app->run();
